@model SingleLineTextDisplayModel
@inject ICofoundryBlockTypeHelper<SingleLineTextDisplayModel> Cofoundry

@{
    Cofoundry.BlockType.UseDescription("Wraps the text in an H3 heading tag");
}
<h3>
    @if (Model == null)
    {
        @:<!-- No text data --->
    }
    else
    {
        @:@Model.Text
    }
</h3>